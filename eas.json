{
  "cli": {
    "version": ">= 12.0.0",
    "appVersionSource": "remote"
  },

  "build": {
    "development": {
      "distribution": "internal",
      "developmentClient": true,
      "channel": "development",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "ios": {
        "credentialsSource": "remote"
      }
    },
    "production": {
      "distribution": "store",
      "channel": "production",
      "autoIncrement": true,
      "ios": {
        "credentialsSource": "remote"
      }
    }
  },

  "submit": {
    "production": {
      "ios": {
        "appleTeamId": "75F4VG5YNK"
        /* ascAppId is optional â€“ omit it and EAS will detect it via bundleIdentifier */
      }
    }
  }
}